TweenManager="*res://scenes/autoloads/tween_manager/tween_manager.tscn"
WeaponLoad="*res://scenes/autoloads/weapon_load/weapon_load.tscn"
Camera="*res://scenes/autoloads/camera/camera.tscn"
HitMark="*res://scenes/autoloads/hit_mark/hit_mark.tscn"
